// Процедура пересчета объема при изменение размера груза.
//
// Параметры:
//  ТекущаяСтрока - Строка - Строка табличной части, строка товарного состава.
//  КоэффициентПересчетаОбъема - Число - Коэффициент.
//
Процедура ГрузРазмерПриИзменении(ТекущаяСтрока, КоэффициентПересчетаОбъема) Экспорт

	ТекущаяСтрока.Объем = ТекущаяСтрока.Ширина * ТекущаяСтрока.Высота * ТекущаяСтрока.Длина * КоэффициентПересчетаОбъема;
	
КонецПроцедуры // ГрузРазмерПриИзменении()

// Пересчет ВГХ грузка.
//
// Параметры:
//  Форма - УправляемаяФорма - Форма объекта.
//  ИдентификаторЭлементаКоллекцииГруза - Строка - Идентификатор.
//  ПараметрВГХ - Структура - Параметры.
//  ЗаполнитьКлассОпасности - Булево - Выполнить заполнение.
//  ЗаполнитьТемпературныйРежим - Булево - Выполнить заполнение.
//
Процедура ПересчитатьХарактеристикиГруза(Форма, ИдентификаторЭлементаКоллекцииГруза, ПараметрВГХ = Неопределено, ЗаполнитьКлассОпасности = Ложь, ЗаполнитьТемпературныйРежим = Ложь) Экспорт

	текОбъект = Форма.Объект;
	
	мсвТоварныйСостав = новый Массив;
	Если текОбъект.ОбщийГруз Тогда
		текЭлемент	= Форма;
		Для каждого текСтрока Из Форма.ТоварныйСостав Цикл
			сткСтрока = Новый Структура("Номенклатура, УпаковкаНоменклатуры, Количество");
			ЗаполнитьЗначенияСвойств(сткСтрока, текСтрока);
			мсвТоварныйСостав.Добавить(сткСтрока);
		КонецЦикла;
	Иначе	
		текЭлемент  = Форма.ГрузовыеМеста.НайтиПоИдентификатору(ИдентификаторЭлементаКоллекцииГруза);
		Для каждого текСтрока Из Форма.ТоварныйСостав Цикл
			Если текСтрока.Идентификатор = текЭлемент.Идентификатор Тогда
				сткСтрока = Новый Структура("Номенклатура, УпаковкаНоменклатуры, Количество");
				ЗаполнитьЗначенияСвойств(сткСтрока, текСтрока);
				мсвТоварныйСостав.Добавить(сткСтрока);
			КонецЕсли;
		КонецЦикла;
	КонецЕсли;
	
	сткХарактеристикиГруза = упЗаданиеНаПеревозкуВызовСервера.ПолучитьХарактеристикиПоТоварам(мсвТоварныйСостав, ЗаполнитьКлассОпасности, ЗаполнитьТемпературныйРежим);
	
	// Масса.
	Если ЗначениеЗаполнено(сткХарактеристикиГруза.Масса) И (сткХарактеристикиГруза.Масса > текЭлемент.Масса ИЛИ ПараметрВГХ = "Масса") Тогда 
		текЭлемент.Масса = сткХарактеристикиГруза.Масса;
	ИначеЕсли ПараметрВГХ = "Масса" Тогда 
		текЭлемент.Масса = 0;
	КонецЕсли;
	
	// Объем.
	Если ЗначениеЗаполнено(сткХарактеристикиГруза.Объем) И (сткХарактеристикиГруза.Объем > текЭлемент.Объем ИЛИ ПараметрВГХ = "Объем") Тогда 
		текЭлемент.Объем = сткХарактеристикиГруза.Объем;
	ИначеЕсли ПараметрВГХ = "Объем" Тогда 
		текЭлемент.Объем = 0;
	КонецЕсли;
	
	// Класс опасности.
	Если ЗаполнитьКлассОпасности И НЕ сткХарактеристикиГруза.КлассОпасности = Неопределено Тогда
		текЭлемент.КлассОпасности	= сткХарактеристикиГруза.КлассОпасности;
	КонецЕсли;
	
	// Температурный режим.
	Если ЗаполнитьТемпературныйРежим И НЕ сткХарактеристикиГруза.ТемпературныйРежим = Неопределено Тогда
		текЭлемент.ТемпературныйРежим	= сткХарактеристикиГруза.ТемпературныйРежим;
	КонецЕсли;

КонецПроцедуры // ПересчитатьХарактеристикиГруза()

// Пересчет итоговой суммы товарного состава.
//
// Параметры:
//  Форма - УправляемаяФорма - Форма объекта.
//  ИдентификаторЭлементаКоллекцииГруза - Строка - Идентификатор.
//
Процедура ПересчитатьИтоговыеСуммы(Форма, ИдентификаторЭлементаКоллекцииГруза) Экспорт
	
	текОбъект = Форма.Объект;
	
	мсвТоварныйСостав = новый Массив;
	сткСтрока = Новый Структура("Номенклатура, УпаковкаНоменклатуры, Количество");
	Если текОбъект.ОбщийГруз Тогда
		текЭлемент	= Форма;
		текЭлемент.Сумма 	= 0;
		текЭлемент.СуммаНДС = 0;
		Для каждого текСтрока Из Форма.ТоварныйСостав Цикл
			текЭлемент.Сумма 	=  текЭлемент.Сумма + текСтрока.Сумма;
			текЭлемент.СуммаНДС =  текЭлемент.СуммаНДС + текСтрока.СуммаНДС;
		КонецЦикла;
	Иначе	
		текЭлемент  = Форма.ГрузовыеМеста.НайтиПоИдентификатору(ИдентификаторЭлементаКоллекцииГруза);
		текЭлемент.Сумма 	= 0;
		текЭлемент.СуммаНДС = 0;
		Для каждого текСтрока Из Форма.ТоварныйСостав Цикл
			Если текСтрока.Идентификатор = текЭлемент.Идентификатор Тогда
				текЭлемент.Сумма 	=  текЭлемент.Сумма + текСтрока.Сумма;
				текЭлемент.СуммаНДС =  текЭлемент.СуммаНДС + текСтрока.СуммаНДС;
			КонецЕсли;
		КонецЦикла;
	КонецЕсли;

	
КонецПроцедуры

