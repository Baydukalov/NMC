#Область ПроверкаЗаполнения

// Проверка корректноти заполнения свойств грузового места.
//
// Параметры:
//  Форма - УправляемаяФорма - Форма объекта.
//  Отказ - Булево - Фиксировать изменения.
//  ПроверятьАдреса - Булево - Выполнить проверку.
//  РазличныеАдресаПолучения - Булево - Это различные адреса получения.
//  РазличныеАдресаОтправления - Булево - Это различные адреса отправления.
//
Процедура ПроверитьГрузовыеМеста(Форма, Отказ, ПроверятьАдреса = Ложь, РазличныеАдресаПолучения = Ложь, РазличныеАдресаОтправления = Ложь) Экспорт

	текОбъект 			= Форма.Объект;
	текГрузовыеМеста 	= Форма.ГрузовыеМеста;
	текТоварныйСостав	= Форма.ТоварныйСостав;
	
	Если текОбъект.ОбщийГруз Тогда 
		
		Если ПроверятьАдреса Тогда 
			Если Не ЗначениеЗаполнено(текОбъект.АдресОтправления) Тогда 
				ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указан адрес отправления'"), текОбъект.Ссылка, "АдресОтправления",, Отказ);
			КонецЕсли;	
			
			Если Не ЗначениеЗаполнено(текОбъект.АдресПолучения) Тогда 
				ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указан адрес получения'"), текОбъект.Ссылка, "АдресПолучения",, Отказ);
			КонецЕсли;	
		КонецЕсли;
		
		Если Не ЗначениеЗаполнено(Форма.ТипГруза) Тогда 
			ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указан тип груза'"), текОбъект.Ссылка, "ТипГруза",, Отказ);
		КонецЕсли;	
		
		Для Каждого текСтрокаСостава Из текТоварныйСостав Цикл 
			Если Не ЗначениеЗаполнено(текСтрокаСостава.Номенклатура) Тогда 
				ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указана номенклатура'"), текОбъект.Ссылка, ОбщегоНазначенияКлиентСервер.ПутьКТабличнойЧасти("ТоварныйСостав", текТоварныйСостав.Индекс(текСтрокаСостава) + 1, "Номенклатура"),, Отказ);
			КонецЕсли;	
			Если Не ЗначениеЗаполнено(текСтрокаСостава.Количество) Тогда 
				ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указано количество'"), текОбъект.Ссылка, ОбщегоНазначенияКлиентСервер.ПутьКТабличнойЧасти("ТоварныйСостав", текТоварныйСостав.Индекс(текСтрокаСостава) + 1, "Количество"),, Отказ);
			КонецЕсли;	
		КонецЦикла;	
	Иначе
		
		Если ПроверятьАдреса Тогда 
			Если Не РазличныеАдресаОтправления Тогда 
				Если Не ЗначениеЗаполнено(текОбъект.АдресОтправления) Тогда 
					ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указан адрес отправления'"), текОбъект.Ссылка, "АдресОтправления",, Отказ);
				КонецЕсли;	
			КонецЕсли;	
			
			Если Не РазличныеАдресаПолучения Тогда 
				Если Не ЗначениеЗаполнено(текОбъект.АдресПолучения) Тогда 
					ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указан адрес получения'"), текОбъект.Ссылка, "АдресПолучения",, Отказ);
				КонецЕсли;	
			КонецЕсли;	
		КонецЕсли;	

		
		Если текГрузовыеМеста.Количество() = 0 Тогда 
			ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указан список грузов'"), текОбъект.Ссылка,"ГрузовыеМеста",, Отказ);
		КонецЕсли;	
		
		Для Каждого текГруз Из текГрузовыеМеста Цикл 
			
			Если ПроверятьАдреса Тогда 
				Если РазличныеАдресаОтправления И Не ЗначениеЗаполнено(текОбъект.АдресОтправления) Тогда 
					Если Не ЗначениеЗаполнено(текГруз.АдресОтправления) Тогда 
						ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указан адрес отправления грузового места'"), текОбъект.Ссылка, ОбщегоНазначенияКлиентСервер.ПутьКТабличнойЧасти("СписокГрузов", текГрузовыеМеста.Индекс(текГруз) + 1, "АдресОтправления"),, Отказ);
					КонецЕсли;	
				КонецЕсли;	
				
				Если РазличныеАдресаПолучения И Не ЗначениеЗаполнено(текОбъект.АдресПолучения) Тогда 
					Если Не ЗначениеЗаполнено(текГруз.АдресПолучения) Тогда 
						ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указан адрес получения грузового места'"), текОбъект.Ссылка, ОбщегоНазначенияКлиентСервер.ПутьКТабличнойЧасти("СписокГрузов", текГрузовыеМеста.Индекс(текГруз) + 1, "АдресПолучения"),, Отказ);
					КонецЕсли;	
				КонецЕсли;	
			КонецЕсли;	
			
			Если Не ЗначениеЗаполнено(текГруз.ТипГруза) Тогда 
				ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указан тип груза'"), текОбъект.Ссылка, ОбщегоНазначенияКлиентСервер.ПутьКТабличнойЧасти("ГрузовыеМеста", текГрузовыеМеста.Индекс(текГруз) + 1, "ТипГруза"),, Отказ);
			КонецЕсли;
			
			текИдентификатор = текГруз.Идентификатор;
			
			мсвСтрокиСостава	= текТоварныйСостав.НайтиСтроки(Новый Структура("Идентификатор", текИдентификатор));
			
			Для каждого текСтрокаСостава Из  мсвСтрокиСостава Цикл
				Если Не ЗначениеЗаполнено(текСтрокаСостава.Номенклатура) Тогда 
					ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указана номенклатура'"), текОбъект.Ссылка, ОбщегоНазначенияКлиентСервер.ПутьКТабличнойЧасти("ТоварныйСостав", текТоварныйСостав.Индекс(текСтрокаСостава) + 1, "Номенклатура"),, Отказ);
				КонецЕсли;	
				Если Не ЗначениеЗаполнено(текСтрокаСостава.Количество) Тогда 
					ОбщегоНазначенияКлиентСервер.СообщитьПользователю(НСтр("ru = 'Не указано количество'"), текОбъект.Ссылка, ОбщегоНазначенияКлиентСервер.ПутьКТабличнойЧасти("ТоварныйСостав", текТоварныйСостав.Индекс(текСтрокаСостава) + 1, "Количество"),, Отказ);
				КонецЕсли;	
			КонецЦикла;
			
			Если Отказ Тогда
				Форма.Элементы.ГрузовыеМеста.ТекущаяСтрока = текГруз.ПолучитьИдентификатор();
				Форма.Элементы.ТоварныйСостав.ОтборСтрок = Новый ФиксированнаяСтруктура("Идентификатор", текИдентификатор);
				Прервать;				
			КонецЕсли;
			
		КонецЦикла;	
	КонецЕсли;	
КонецПроцедуры

// Процедура поиска дублей таблицы с ГСМ.
//
// Параметры:
//  ТекущаяСтрока	 - Строка	 - Строка табличной части, строка данных.
//  Форма			 - УправляемаяФорма	 - Форма объекта.
//  ИмяТЧ			 - Строка			 - имя табличной части.
//  Отказ			 - Булево			 - Фиксировать изменения.
//
Процедура ЕстьДублиСтрокВТабличнойЧасти(ТекущаяСтрока, Форма, ИмяТЧ, Отказ) Экспорт
	
	Если Форма.ИмяФормы = "Документ.упПутевойЛист.Форма.ФормаДокумента" Тогда
		Если ИмяТЧ = "ГСМ" Тогда
			сткОтбора = Новый Структура("ВидГСМ, ДопОборудование",ТекущаяСтрока.ВидГСМ, ТекущаяСтрока.ДопОборудование);
			ТекстСообщения = Нстр("ru = 'По виду гсм ""%1"" и ""%2"" уже есть строка'");
			ТранспортноеСредство = ?(ЗначениеЗаполнено(ТекущаяСтрока.ДопОборудование), ТекущаяСтрока.ДопОборудование, Форма.Объект.ТранспортноеСредство);
			ТекстСообщения = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ТекстСообщения, ТекущаяСтрока.ВидГСМ, ТранспортноеСредство);
		ИначеЕсли ИмяТЧ = "ДополнительноеОборудование" Тогда	
			сткОтбора = Новый Структура("ДополнительноеОборудование", ТекущаяСтрока.ДополнительноеОборудование);
			ТекстСообщения = Нстр("ru = 'В табличной части уже есть строка с доп. оборудованием ""%1""'");
			ТекстСообщения = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ТекстСообщения, ТекущаяСтрока.ДополнительноеОборудование);
		ИначеЕсли ИмяТЧ = "ГСМДополнительноеОборудование" Тогда	
			сткОтбора = Новый Структура("ДополнительноеОборудование, ВидГСМ", ТекущаяСтрока.ДополнительноеОборудование, ТекущаяСтрока.ВидГСМ);
			ТекстСообщения = Нстр("ru = 'В табличной части уже есть строка с доп. оборудованием ""%1"" и видом гсм ""%2""'");
			ТекстСообщения = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ТекстСообщения, ТекущаяСтрока.ДополнительноеОборудование, ТекущаяСтрока.ВидГСМ);	
		КонецЕсли;
	КонецЕсли;
	
	ДанныеФормыКоллекция = Форма.Объект[ИмяТЧ];	
	мсвСтроки = ДанныеФормыКоллекция.НайтиСтроки(сткОтбора);
	Если мсвСтроки.Количество() > 1 Тогда
		ОбщегоНазначенияКлиентСервер.СообщитьПользователю(ТекстСообщения,,,,Отказ);
	КонецЕсли;
	
КонецПроцедуры


#КонецОбласти

#КонецОбласти 