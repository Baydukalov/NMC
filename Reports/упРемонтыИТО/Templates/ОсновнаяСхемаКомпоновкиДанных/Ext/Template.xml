		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	упРемонтИТОТС.Ссылка,
	упРемонтИТОТС.ВидРемонта,
	упРемонтИТОТС.ТранспортноеСредство,
	ЕСТЬNULL(СтатусыРемонтов.Статус, ЗНАЧЕНИЕ(Перечисление.упСтатусыРемонтаИТОТС.Новый)) КАК Статус,
	упРемонтИТОТС.СуммаРасходыПланВалюта КАК СуммаРасходыПлан
ПОМЕСТИТЬ вт_Данные
ИЗ
	Документ.упРемонтИТОТС КАК упРемонтИТОТС
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.упСтатусыРемонтовИТОТС.СрезПоследних({(&amp;КонецПериода)}, ) КАК СтатусыРемонтов
		ПО упРемонтИТОТС.Ссылка = СтатусыРемонтов.Документ
ГДЕ
	упРемонтИТОТС.Проведен
	И упРемонтИТОТС.Дата МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
;

