ИЗ
	Документ.упРемонтИТОТС.Прицепы КАК Прицепы
ГДЕ
	Прицепы.Ссылка В
			(ВЫБРАТЬ
				вт_Данные.Ссылка
			ИЗ
				вт_Данные КАК вт_Данные)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	АгрегатыС.Ссылка,
	АгрегатыС.Агрегат,
	АгрегатыС.Склад,
	АгрегатыС.Количество,
	АгрегатыС.ТипОперацииМонтажа
ИЗ
	Документ.упРемонтИТОТС.ШиныУзлыАгрегатыСобственные КАК АгрегатыС
ГДЕ
	АгрегатыС.Ссылка В
			(ВЫБРАТЬ
				вт_Данные.Ссылка
			ИЗ
				вт_Данные КАК вт_Данные)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	вт_Приобретенные.Ссылка,
	вт_Приобретенные.Материал,
	вт_Приобретенные.СтатьяРасходов,
	вт_Приобретенные.Сумма КАК СуммаП,
	вт_Приобретенные.Количество КАК КоличествоП,
	ЗНАЧЕНИЕ(Справочник.упСклады.ПустаяСсылка) КАК Склад,
	0 КАК КоличествоС
ПОМЕСТИТЬ вт_Таблицы
ИЗ
	вт_Приобретенные КАК вт_Приобретенные

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	вт_Собственные.Ссылка,
	вт_Собственные.Материал,
	вт_Собственные.СтатьяРасходов,
	0,
	0,
	вт_Собственные.Склад,
	вт_Собственные.Количество
ИЗ
	вт_Собственные КАК вт_Собственные
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	вт_Работы.Ссылка КАК Ремонт,
	ЕСТЬNULL(вт_Работы.РаботаПоРемонту, ЗНАЧЕНИЕ(Справочник.упРаботыПоРемонту.ПустаяСсылка)) КАК ВыполненыеРаботы,
	ЕСТЬNULL(вт_Работы.СуммаСНДС, 0) КАК СуммаРасходыФакт,
	ЗНАЧЕНИЕ(Справочник.упЗапчасти.ПустаяСсылка) КАК Материал,
	ЕСТЬNULL(вт_Работы.СтатьяРасходов, ЗНАЧЕНИЕ(Справочник.упСтатьиДоходовРасходов.ПустаяСсылка)) КАК СтатьяРасходов,
	0 КАК СуммаП,
	0 КАК КоличествоП,
	ЗНАЧЕНИЕ(Справочник.упСклады.ПустаяСсылка) КАК Склад,
	0 КАК КоличествоС
ПОМЕСТИТЬ вт_Результат
ИЗ
	вт_Работы КАК вт_Работы

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	вт_Таблицы.Ссылка,
	ЕСТЬNULL(вт_Таблицы.Материал, ЗНАЧЕНИЕ(Справочник.упЗапчасти.ПустаяСсылка)),
	0,
	ЗНАЧЕНИЕ(Справочник.упРаботыПоРемонту.ПустаяСсылка),
	ЕСТЬNULL(вт_Таблицы.СтатьяРасходов, ЗНАЧЕНИЕ(Справочник.упСтатьиДоходовРасходов.ПустаяСсылка)),
	ЕСТЬNULL(вт_Таблицы.СуммаП, 0),
	ЕСТЬNULL(вт_Таблицы.КоличествоП, 0),
	ЕСТЬNULL(вт_Таблицы.Склад, ЗНАЧЕНИЕ(Справочник.упСклады.ПустаяСсылка)),
	ЕСТЬNULL(вт_Таблицы.КоличествоС, 0)
ИЗ
	вт_Таблицы КАК вт_Таблицы
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	вт_Данные.ТранспортноеСредство,
	вт_Данные.ВидРемонта,
	вт_Данные.Ссылка КАК Ремонт,
	вт_Данные.Статус,
	ЕСТЬNULL(вт_Результат.ВыполненыеРаботы, ЗНАЧЕНИЕ(Справочник.упРаботыПоРемонту.ПустаяСсылка)) КАК ВыполненыеРаботы,
	вт_Данные.СуммаРасходыПлан КАК СуммаРасходыПлан,
	ЕСТЬNULL(вт_Результат.СуммаРасходыФакт, 0) КАК СуммаРасходыФакт,
	ЕСТЬNULL(вт_Результат.Материал, ЗНАЧЕНИЕ(Справочник.упЗапчасти.ПустаяСсылка)) КАК Материал,
	ЕСТЬNULL(вт_Результат.СтатьяРасходов, ЗНАЧЕНИЕ(Справочник.упСтатьиДоходовРасходов.ПустаяСсылка)) КАК СтатьяРасходов,
	ЕСТЬNULL(вт_Результат.СуммаП, 0) КАК СуммаП,
	ЕСТЬNULL(вт_Результат.КоличествоП, 0) КАК КоличествоП,
	ЕСТЬNULL(вт_Результат.Склад, ЗНАЧЕНИЕ(Справочник.упСклады.ПустаяСсылка)) КАК Склад,
	ЕСТЬNULL(вт_Результат.КоличествоС, 0) КАК КоличествоС
ИЗ
	вт_Данные КАК вт_Данные
		ЛЕВОЕ СОЕДИНЕНИЕ вт_Результат КАК вт_Результат
		ПО вт_Данные.Ссылка = вт_Результат.Ремонт
			И (НЕ вт_Данные.Статус = ЗНАЧЕНИЕ(Перечисление.упСтатусыРемонтаИТОТС.Отменен))</query>
	</dataSet>
	<calculatedField>
		<dataPath>СуммаРасходыПлан1</dataPath>
		<expression>0</expression>
		<title xsi:type="v8:LocalStringType">
			<v8:item>
				<v8:lang>ru</v8:lang>
				<v8:content>Сумма расходы план1</v8:content>
			</v8:item>
		</title>
	</calculatedField>
	<totalField>
		<dataPath>КоличествоП</dataPath>
		<expression>Сумма(КоличествоП)</expression>
	</totalField>
	<totalField>
		<dataPath>КоличествоС</dataPath>
		<expression>Сумма(КоличествоС)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаП</dataPath>
		<expression>Сумма(СуммаП)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаРасходыПлан</dataPath>
		<expression>Сумма(СуммаРасходыПлан)</expression>
	</totalField>
	<totalField>
		<dataPath>СуммаРасходыФакт</dataPath>
		<expression>Сумма(СуммаРасходыФакт)</expression>
		<group>ВидРемонта</group>
		<group>ВыполненыеРаботы</group>
		<group>Ремонт</group>
		<group>ТранспортноеСредство</group>
	</totalField>
	<totalField>
		<dataPath>СуммаРасходыПлан1</dataPath>
		<expression>Сумма(ВычислитьВыражениеСГруппировкойМассив("Максимум(СуммаРасходыПлан)","Ремонт"))</expression>
		<group>ВидРемонта</group>
		<group>Ремонт</group>
		<group>Статус</group>
		<group>ТранспортноеСредство</group>
		<group>ОбщийИтог</group>
	</totalField>
