		<Attribute name="ЗаявкиНаТС" id="7">
			<Title>
				<v8:item>
					<v8:lang>ru</v8:lang>
					<v8:content>Заявки на ТС</v8:content>
				</v8:item>
			</Title>
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<UseAlways>
				<Field>~ЗаявкиНаТС.Ref</Field>
				<Field>~ЗаявкиНаТС.Статус</Field>
				<Field>ЗаявкиНаТС.НачалоВыполненияФакт</Field>
				<Field>ЗаявкиНаТС.ОкончаниеВыполненияФакт</Field>
				<Field>ЗаявкиНаТС.ПлановоеНачалоВыполнения</Field>
				<Field>ЗаявкиНаТС.ПлановоеОкончаниеВыполнения</Field>
				<Field>ЗаявкиНаТС.ПодтверждениеЗаявкиНаТС</Field>
				<Field>ЗаявкиНаТС.Предложение</Field>
				<Field>ЗаявкиНаТС.Рейс</Field>
				<Field>ЗаявкиНаТС.Ссылка</Field>
				<Field>ЗаявкиНаТС.СтатусЗаявки</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	упЗаявкаНаТС.Ссылка КАК Ссылка,
	упЗаявкаНаТС.Тендер КАК Тендер,
	упЗаявкаНаТС.Перевозчик КАК Перевозчик,
	упЗаявкаНаТС.Соглашение КАК Соглашение,
	упЗаявкаНаТС.ТипТС КАК ТипТС,
	ВЫБОР
		КОГДА упСтатусыЗаявокНаТССрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.упСтатусыЗаявкиНаТС.Отправлена)
			ТОГДА ЗНАЧЕНИЕ(Перечисление.упСтатусыЗаявкиНаТСДляПеревозчика.Новая)
		КОГДА упСтатусыЗаявокНаТССрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.упСтатусыЗаявкиНаТС.ПолученоПредложение)
			ТОГДА ЗНАЧЕНИЕ(Перечисление.упСтатусыЗаявкиНаТСДляПеревозчика.ОтправленоПредложение)
		КОГДА упСтатусыЗаявокНаТССрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.упСтатусыЗаявкиНаТС.Подтверждена)
			ТОГДА ЗНАЧЕНИЕ(Перечисление.упСтатусыЗаявкиНаТСДляПеревозчика.Подтверждена)
		КОГДА упСтатусыЗаявокНаТССрезПоследних.Статус = ЗНАЧЕНИЕ(Перечисление.упСтатусыЗаявкиНаТС.Отклонена)
			ТОГДА ЗНАЧЕНИЕ(Перечисление.упСтатусыЗаявкиНаТСДляПеревозчика.Отклонена)
	КОНЕЦ КАК СтатусЗаявки,
	упСтатусыЗаявокНаТССрезПоследних.Статус.Порядок КАК ИндексКартинки,
	упЗаявкаНаТС.Рейс КАК Рейс,
	ЕСТЬNULL(упЗаявкаНаТС.Объем, 0) КАК Объем,
	ЕСТЬNULL(упЗаявкаНаТС.Масса, 0) КАК Масса,
	ЕСТЬNULL(упЗаявкаНаТС.КоличествоМест, 0) КАК КоличествоМест,
	ЕСТЬNULL(упЗаявкаНаТС.КоличествоЗагрузочныхМетров, 0) КАК КоличествоЗагрузочныхМетров,
	ЕСТЬNULL(упПлановыеПараметрыРейса.ЗонаПогрузки, ЗНАЧЕНИЕ(Справочник.упГеографическиеЗоны.ПустаяСсылка)) КАК ЗонаПогрузки,
	ЕСТЬNULL(упПлановыеПараметрыРейса.ЗонаРазгрузки, ЗНАЧЕНИЕ(Справочник.упГеографическиеЗоны.ПустаяСсылка)) КАК ЗонаРазгрузки,
	ЕСТЬNULL(упЗаявкаНаТС.НачалоРейса, ДАТАВРЕМЯ(1, 1, 1)) КАК ПлановоеНачалоВыполнения,
	ЕСТЬNULL(упЗаявкаНаТС.ОкончаниеРейса, ДАТАВРЕМЯ(1, 1, 1)) КАК ПлановоеОкончаниеВыполнения,
	ЕСТЬNULL(упФактическиеПараметрыРейса.Объем, 0) КАК ОбъемФакт,
	ЕСТЬNULL(упФактическиеПараметрыРейса.Масса, 0) КАК МассаФакт,
	ЕСТЬNULL(упФактическиеПараметрыРейса.КоличествоМест, 0) КАК КоличествоМестФакт,
	ЕСТЬNULL(упФактическиеПараметрыРейса.КоличествоЗагрузочныхМетров, 0) КАК КоличествоЗагрузочныхМетровФакт,
